function getobstruct(from, to, mod, block)
	local newtovector = to
	if mod == nil then mod = game:service(&quot;Workspace&quot;) end
	if mod == gun.Parent or mod.Name == &quot;Laser&quot; then return to, block end
	if mod:isA(&quot;BasePart&quot;) then
		local slx = mod.Position.x - (mod.Size.x/2)
		local sbx = mod.Position.x + (mod.Size.x/2)
		local sly = mod.Position.y - (mod.Size.y/2)
		local sby = mod.Position.y + (mod.Size.y/2)
		local slz = mod.Position.z - (mod.Size.z/2)
		local sbz = mod.Position.z + (mod.Size.z/2)
		local ascentvector = -CFrame.new((from + to)/2,from).lookVector
		for i = 1, math.ceil((from - to).magnitude) do
			local cvec = from + (ascentvector * i)
			if cvec.x &gt; slx and cvec.x &lt; sbx then
				if cvec.y &gt; sly and cvec.y &lt; sby then
					if cvec.z &gt; slz and cvec.z &lt; sbz then
						newtovector = cvec
						block = mod
						print(&quot;Set block&quot;)
						break
					end
				end
			end
		end
	end
	local st = mod:GetChildren()
	for u = 1, #st do
		newtovector, block = getobstruct(from, newtovector, st[u], block)
		print(block)
	end
	return newtovector, block
end

function makeLazer(from, to)
	local P = Instance.new(&quot;Part&quot;) 
	P.formFactor = 0 
	P.Size = Vector3.new(1,1,1) 
	local M = Instance.new(&quot;BlockMesh&quot;)
	M.Scale = Vector3.new(width.Value,width.Value,(from - to).magnitude)
	M.Parent = P
	P.Name = &quot;Laser&quot; 
	P.CFrame = CFrame.new((from + to)/2,from) 
	P.Anchored = true
	P.CanCollide = false 
	P.Locked = true 
	P.BottomSurface = &quot;Smooth&quot; 
	P.TopSurface = &quot;Smooth&quot; 
	P.BrickColor = color.Value
	P.Parent = game:service(&quot;Workspace&quot;)
	return P
end

function maketag()
	local tag = Instance.new(&quot;ObjectValue&quot;)
	tag.Name = &quot;creator&quot;
	tag.Value = game:service(&quot;Players&quot;):playerFromCharacter(gun.Parent)
	return tag
end

function GetHumanoid(model)
	local h = model:GetChildren()
	for i = 1, #h do
		if h[i].className == &quot;Humanoid&quot; then return h[i] end
	end
	return nil
end

function getoffset()
	return Vector3.new(math.random()-0.5,math.random()-0.5,math.random()-0.5).unit * 2 * accuracyoffset.Value
end

function fireatpos(pos, targ)
	enabled = false
	local from = handle.Position + (handle.CFrame.lookVector * (handle.Size.z/2))
	local dist = (from-pos).magnitude
	if range.Value &gt; 2500 then h.Text = &quot;Error: Range Lethal!&quot; return end
	for i = 1, bpc.Value do
		pos = pos + ((CFrame.new((from + pos)/2,from).lookVector).unit*(dist-range.Value)+getoffset()) -- Resize to 500 studs max and re-accuracize?
		local to, obj = getobstruct(from, pos, nil, nil)
		dist = (from-to).magnitude
		local laser = makeLazer(from, to)
		if obj ~= nil then
			local V = obj:findFirstChild(&quot;Shot&quot;)
			if V ~= nil then
				if V.className == &quot;IntValue&quot; then
					V.Value = V.Value + 1
				end
			end
			if obj.Parent ~= nil then
				local h = GetHumanoid(obj.Parent)
				if h ~= nil then
					local t = maketag()
					t.Parent = h
					h:TakeDamage(damage.Value)
					coroutine.resume(coroutine.create(function()
						wait(0.1)
						t:Remove()
					end))
				end
			end
		end
		local sc = script.LazerScript:Clone()
		sc.Disabled = false
		sc.Parent = laser
	end
	wait(1/shotspersecond.Value)
	enabled = true
end

function ue()
	h.Text = bullettype.. &quot;: &quot;.. energy.. &quot;/&quot;.. maxenergy.Value
	h.Parent = game:service(&quot;Players&quot;):playerFromCharacter(gun.Parent)
end

function Button1Down(mouse)
	if energy &lt;= 0 or enabled == false then return end
	held = true
	reloading = false
	if autogun.Value then
		while energy &gt; 0 and held == true do
			energy = energy - 1
			ue()
			fireatpos(mouse.Hit.p)
		end
	else
		energy = energy - 1
		ue()
		fireatpos(mouse.Hit.p)
	end
	Button1Up(mouse)
end

function Button1Up(mouse)
	held = false
	if reloading == true then return end
	reloading = true
	mouse.Icon = &quot;rbxasset://textures\\GunWaitCursor.png&quot;
	currentreloadid = currentreloadid+1
	local r = currentreloadid
	while held == false and reloading == true and energy &lt; maxenergy.Value do
		wait(reloadtime.Value)
		if held == true or reloading == false or currentreloadid ~= r then break end
		energy = energy + 1
		ue()
	end
	mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;
	enabled = true
	reloading = false
end

function select(mouse)
	mouse.Icon = &quot;rbxasset://textures\\GunCursor.png&quot;
	mouse.Button1Down:connect(function() Button1Down(mouse) end)
	mouse.Button1Up:connect(function() Button1Up(mouse) end)
end

