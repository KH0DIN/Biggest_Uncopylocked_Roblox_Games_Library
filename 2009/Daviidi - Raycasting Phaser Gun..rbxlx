						<string name="Source">debounce = {}
creator = script.Parent.creator

damage = 20

function Touch(hit)
	if hit.Parent == nil then return end
	if hit.Parent:findFirstChild(&quot;Humanoid&quot;) == nil then return end
	if game:service(&quot;Players&quot;):playerFromCharacter(hit.Parent) == creator.Value then return end
	for i = 1, #debounce do
		if debounce[i] == hit.Parent.Humanoid then return end
	end
	table.insert(debounce, hit.Parent.Humanoid)
	if hit.Parent.Humanoid.Health - damage &lt;= 0 then
		creator:Clone().Parent = hit.Parent.Humanoid
	end
	hit.Parent.Humanoid:TakeDamage(damage)
	wait(1)
	if hit.Parent.Humanoid:findFirstChild(&quot;creator&quot;) ~= nil then hit.Parent.Humanoid.creator:Remove() end
end

