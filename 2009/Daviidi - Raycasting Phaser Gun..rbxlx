					<string name="Source">gun = script.Parent
handle = gun.Gun

function getobstruct(from, to, mod)
	local newtovector = to
	if mod == nil then mod = game:service(&quot;Workspace&quot;) end
	if mod == handle or mod.Name == &quot;Laser&quot; then return to end
	if mod.className == &quot;Part&quot; or mod.className == &quot;Truss&quot; or mod.className == &quot;SpawnLocation&quot; or mod.className == &quot;Seat&quot; or mod.className == &quot;VehicleSeat&quot; then
		local slx = mod.Position.x - (mod.Size.x/2)
		local sbx = mod.Position.x + (mod.Size.x/2)
		local sly = mod.Position.y - (mod.Size.y/2)
		local sby = mod.Position.y + (mod.Size.y/2)
		local slz = mod.Position.z - (mod.Size.z/2)
		local sbz = mod.Position.z + (mod.Size.z/2)
		local ascentvector = -CFrame.new((from + to)/2,from).lookVector
		for i = 1, math.ceil((from - to).magnitude) do
			local cvec = from + (ascentvector * i)
			if cvec.x &gt; slx and cvec.x &lt; sbx then
				if cvec.y &gt; sly and cvec.y &lt; sby then
					if cvec.z &gt; slz and cvec.z &lt; sbz then
						newtovector = cvec
						break
					end
				end
			end
		end
	end
	local st = mod:GetChildren()
	for u = 1, #st do
		newtovector = getobstruct(from, newtovector, st[u])
	end
	return newtovector
end

function makeLazer(from, to)
	local P = Instance.new(&quot;Part&quot;) 
	P.formFactor = 0 
	P.Size = Vector3.new(1,1,(from - to).magnitude) 
	P.Name = &quot;Laser&quot; 
	P.CFrame = CFrame.new((from + to)/2,from) 
	P.Anchored = true
	P.CanCollide = false 
	P.Locked = true 
	P.BottomSurface = &quot;Smooth&quot; 
	P.TopSurface = &quot;Smooth&quot; 
	P.BrickColor = BrickColor.new(21)
	P.Parent = game:service(&quot;Workspace&quot;)
	return P
end

function fireatpos(pos, maker)
	local from = handle.Position + (handle.CFrame.lookVector * (handle.Size.z/2))
	local dist = (from-pos).magnitude
	if dist &gt; 500 then
		pos = pos + ((CFrame.new((from + pos)/2,from).lookVector)*(dist-500))
	end
	local to = getobstruct(from, pos)
	dist = (from-to).magnitude
	local laser = makeLazer(from, to)
	local tag = Instance.new(&quot;ObjectValue&quot;)
	tag.Name = &quot;creator&quot;
	tag.Value = maker
	if tag.Value ~= nil then tag.Parent = laser end
	local sc = script.LazerScript:Clone()
	sc.Disabled = false
	sc.Parent = laser
	for t = 0, 1, 0.1 do
		wait()
		laser.Transparency = t
	end
	laser:Remove()
end

script.Parent.Trigger.Touched:connect(function(hit)
	if hit.Parent == nil then return end
	local maker = game:service(&quot;Players&quot;):playerFromCharacter(hit.Parent)
	if maker == nil then return end
	fireatpos(handle.CFrame.lookVector * 500, maker)
end)</string>
